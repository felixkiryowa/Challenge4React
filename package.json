{
  "name": "reactwebpack",
  "version": "1.0.0",
  "description": "Boliler plate for react",
  "main": "index.js",
  "scripts": {
    "test": "react-scripts test",
    "start": "webpack-dev-server --mode development --open --hot",
    "build": "webpack --mode production",
    "test:coverage": "react-scripts test --coverage",
    "test:coverage:watch": "react-scripts test --watchAll -u --coverage",
    "test:coverage:ci": "react-scripts test --coverage --maxWorkers 2",
    "scss": "node-sass scss/style.scss public/dest/style.css --watch"
  },
  "author": "Francis Kiryowa",
  "license": "MIT",
  "dependencies": {
    "css-loader": "^2.1.1",
    "express": "^4.16.4",
    "node-sass": "^4.12.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-redux": "^7.0.2",
    "react-router-dom": "^5.0.0",
    "react-scripts": "^3.0.0",
    "redux": "^4.0.1",
    "redux-devtools-extension": "^2.13.8",
    "redux-thunk": "^2.3.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "bootstrap": "^4.0.0-alpha.6",
    "codecov": "^3.3.0",
    "coveralls": "^3.0.3",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.12.1",
    "enzyme-to-json": "^3.3.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "jquery": "^3.4.0",
    "popper": "^1.0.1",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "jest": {
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
